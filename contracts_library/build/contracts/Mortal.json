  "source": "import \"./Owned.sol\";\n\ncontract Mortal is Owned {\n    event mortalCalled(string);\n    function kill() public {\n        emit mortalCalled(\"mortalCalled\");\n        if (msg.sender == owner) selfdestruct(owner);\n    }\n}",
